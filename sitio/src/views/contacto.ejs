<!DOCTYPE html>
<html lang="es">


<!-- Include Head -->
<%- include('partials/head', {title: 'Carrito'}) %>

<body class="contact">

  <!-- Header -->
	<%- include('partials/header') %>
	<!-- /Header -->

  <!--  order section starts  -->

  <section class="order" id="order">
	<h1 class="heading">Contacto</h1>

	<form action="">

		<div class="inputBox">
			<div class="input">
				<span>Nombre</span>
				<input type="text" placeholder="Coloca tu nombre">
			</div>
			<div class="input">
				<span>Telefono</span>
				<input type="number" placeholder="Coloca tu telefono">
			</div>
		</div>
		<div class="inputBox">
			<div class="input">
				<span>Email</span>
				<input type="email" placeholder="Coloca tu correo">
			</div>
			<div class="input">
				<span>Fecha</span>
				<input type="datetime-local">
			</div>
		</div>
		<div class="inputBox">
			<div class="input">
				<span>Consulta</span>
				<textarea type="" placeholder="Realiza tu consulta"></textarea>
			</div>
		</div>

		<input type="submit" value="Enviar" class="btn width">

	</form>

</section>

<!--  order section ends  -->

<!-- footer -->
<%- include('partials/footer') %>
<!-- /footer -->

<script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>

    <!-- custom js file link  -->
    <script src="/js/script.js"></script>

</body>
</html>